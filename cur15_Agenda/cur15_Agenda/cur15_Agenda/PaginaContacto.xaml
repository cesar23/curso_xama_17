<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="cur15_Agenda.PaginaContacto" Padding="10" Title="Contacto">
    
        <StackLayout>
            <Label Text="{Binding Nombre}" Font="Large" TextColor="White" />
            <Label Text="{Binding Telefono}" Font="Small" TextColor="#BDDBBB" />
            <Label Text="{Binding Direccion}" Font="Small" TextColor="#FAFAFA" />
    </StackLayout>
    
</ContentPage>